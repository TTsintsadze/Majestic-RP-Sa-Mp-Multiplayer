CMD:refill(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
    if(PlayerInfo[playerid][pJob] != 2) return SendClientMessage(playerid, COLOR_GREY, "Tqven Ar Xart Meqanikosi");
	if(gcontract[playerid] != 1) return SendClientMessage(playerid, COLOR_GREY, "Tqven Ar Gaqvt Kontraqti");
	if(sscanf(params, "u", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /refill [ID Motamashis]");
	if(GetPlayerState(params[0]) == PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, COLOR_GREY, "Motamashe Ar Aris Manqanashi");
	if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 525) return SendClientMessage(playerid, COLOR_GREY, "Tqven Ar Xart Meqanikosis Manqanashi");
	if(!ProxDetectorS(8.0, playerid, params[0])) return true;
	if(params[0] == playerid) return SendClientMessage(playerid, COLOR_GREY, "Tqven Miutitet Tqveni ID");
	format(YCMDstr, sizeof(YCMDstr), "Tqven Shestavazet Motamashes %s Benzini. Safasuri %d$.",Name(params[0]),cenabenza[playerid]);
	SendClientMessage(playerid, 0x6495EDFF, YCMDstr);
	format(YCMDstr, sizeof(YCMDstr), "Meqaniki %s Gtavazobt Benzins. Safasuri %d$, (( /accept refill ))",Name(playerid),cenabenza[playerid]);
	SendClientMessage(params[0], 0x6495EDFF, YCMDstr);
	RefillOffer[params[0]] = playerid;
	RefillPrice[params[0]] = cenabenza[playerid];
	return true;
}

