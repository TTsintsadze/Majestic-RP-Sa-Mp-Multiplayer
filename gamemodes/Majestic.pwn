		else if(memberr == 6) { format(string, sizeof(string), "- {8B0000}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
		else if(memberr == 7) { format(string, sizeof(string), "- {C0FF3E}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
		else if(memberr == 12) { format(string, sizeof(string), "- {CD00CD}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
		else if(memberr == 13) { format(string, sizeof(string), "- {FFFF00}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
		else if(memberr == 14) { format(string, sizeof(string), "- {008B8B}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
  		else if(memberr == 15) { format(string, sizeof(string), "- {00CD00}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
  		else if(memberr == 16) { format(string, sizeof(string), "- {40848B}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
  		else if(memberr == 17) { format(string, sizeof(string), "- {00F5FF}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
      	else if(memberr == 18) { format(string, sizeof(string), "- {6959CD}%s(%d): {FFFFFF}%s", Name(playerid),playerid, text); }
        else if(AdminDuty[playerid])
		{
      		format(string, sizeof(string), "{FF0000}%s{FFFFFF}: (( %s ))",Name(playerid),text);
  		}
  		else
  		{
		    format(string, sizeof(string), "- %s(%d): %s", Name(playerid),playerid, text);
