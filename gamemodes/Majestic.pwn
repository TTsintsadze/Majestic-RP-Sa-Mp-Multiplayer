					CountOnZone[GZInfo[i][gNapad]] = 0;
					SendFamilyMessage(GZInfo[i][gNapad], 0x00B953AA,"{FFFFFF}Gilocavt, Tqven Daipyarit Axali Teritoria");
					SendFamilyMessage(GZInfo[i][gFrakVlad], 0xC56565AA,"{FF0000}Samwuxarod Tqven Dakarget Teritoriaze Kontroli.");
					format(capt_str, sizeof(capt_str), "{FF0000}[GhettoInfo] {FFFFFF}Teraze Sauketeso Motamashe Iyo: %s[%d] / [Kills: %d]", Name(highscoreowner), highscoreowner, highscore);
					SendFamilyMessage(GZInfo[i][gFrakVlad],COLOR_WHITE,capt_str);
					SendFamilyMessage(GZInfo[i][gNapad],COLOR_WHITE,capt_str);
