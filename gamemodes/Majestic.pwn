CMD:sms(playerid, params[])
{
    if(PlayerInfo[playerid][pLogin] == 0) return true;
	if(sscanf(params, "ds[64]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Sheiyvanet: /sms [ID Motamashis] [Text]");
	if(PlayerInfo[playerid][pPhone1] == 0) return SendClientMessage(playerid, COLOR_GREY, "Tqven Ar Gaqvt Mobiluri Telefoni Misi Shedzena Shegidzliat Telefonis Magaziashi Romelsac Naxavt GPS.");
	if(PlayerInfo[playerid][pJailTime] > 0)return SendClientMessage(playerid, COLOR_WHITE, "Cixeshi Ver Gamoiyeneb Telefons");
	if(PlayerInfo[playerid][pMuted] == 1) return SendErrorMessage(playerid, "Tkven Gadevt Chatis Bani!");
	if(PlayerInfo[params[0]][pLogin] != 1) return SendClientMessage(playerid, COLOR_WHITE, "Am Motamashes Ar Aqvs Telefoni!"); // avtorizacia
	if(PlayerInfo[params[0]][pPhone1] == 0) return SendClientMessage(playerid, -1, "Am Motamashes Ar Aqvs Telefoni!");
	if(PhoneOnline[playerid] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Tqven Gatishuli Gaqvt Mobiluri");
    if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_CUFFED) return SendClientMessage(playerid,0xFFFFFFFF,"Xelborkilebi gadevs");
	if(IsPlayerInRangeOfPoint(playerid, 80.0, 1943.5758,1017.9280,993.5148) && PlayerInfo[playerid][pAdmin] < 10) return SendClientMessage(playerid, COLOR_WHITE, "Cazinoshi Telefonis Gamoyeneba Akrdzalulia");
	if(IsIP(params[1]))
	{
		if(PlayerInfo[playerid][pLogin] == 0) return true;
		PlayerInfo[playerid][pMuteTime] = 600;
		PlayerInfo[playerid][pMuted] = 1;
		SendClientMessage(playerid, COLOR_WHITE, "Tqven Dagedot 10 Wutiani Chatis Bani Cifrebis Daweristvis.");
		SetPlayerChatBubble(playerid, "Daedo Chatis Bani",0xDD90FFFF,30.0,10000);
		return false;
	}
	if(Rekl[playerid] == 1 && strfind(params[1],"server",true)!=-1 || Rekl[playerid] == 1 && strfind(params[1],"serveri",true)!=-1
			|| Rekl[playerid] == 1 && strfind(params[1],"serverze",true)!=-1 || Rekl[playerid] == 1 && strfind(params[1],"serveria",true)!=-1
			|| Rekl[playerid] == 1 && strfind(params[1],".com",true)!=-1)
	{
		format(YCMDstr, sizeof(YCMDstr), "[MUTE][SMS] %s[%d] %s",Name(playerid),playerid,params[1]);
		ABroadCast(COLOR_LIGHTRED, YCMDstr, 1);
		format(YCMDstr,sizeof(YCMDstr),"Tqven Dagedot Chatis Bani 10 Wutit.");
		SendClientMessage(playerid, COLOR_LIGHTRED, YCMDstr);
		PlayerInfo[playerid][pMuteTime] = 600;
		PlayerInfo[playerid][pMuted] = 1;
		return true;
	}
	GetPlayerPos(playerid,CallInfo[playerid][callx],CallInfo[playerid][cally],CallInfo[playerid][callz]);
	CallInfo[playerid][callused] = 1;
	switch (params[0])
	{
	case 11888:
		{
			if(smsls == 0) return SendClientMessage(playerid, COLOR_GREY, "Sms Migeba Gatishulia");
			if(PlayerInfo[playerid][pMobile] < smspricels) return SendClientMessage(playerid, COLOR_GREY, "[Operatori]: Tqvens angarishze tanxa araris");
			format(YCMDstr, sizeof(YCMDstr), "[Live] %s. Gamogzavna: %s[%d]",params[1],Name(playerid), playerid);
			SendFamilyMessage(16, COLOR_YELLOW, YCMDstr);
			format(YCMDstr, sizeof(YCMDstr), "SMS: %s. Mimgebi: LS NEWS",params[1]);
			SendClientMessage(playerid,  COLOR_YELLOW, YCMDstr);
			PlayerInfo[playerid][pMobile] -= smspricels;
			FracBank[0][fLsnews] += smspricels;
		}
	}
	foreach(new i:Player)
	{
		if(params[0] == PlayerInfo[i][pPnumber])
		{
			if(PhoneOnline[i] > 0)
			{
				SendClientMessage(playerid, COLOR_GREY, "Sms Shetyobineba Ver Gaigzavna Radgan Motamashes Gatishuli Aqvs Mobiluri.");
				return true;
			}
			if(PlayerInfo[playerid][pMobile] < 1) return SendClientMessage(playerid, COLOR_GREY, "Tqven Ar Gaqvt Mobiluris Angarishze Sakmarisi Tanxa");
			PlayerInfo[playerid][pMobile] -= 20;
			format(YCMDstr, sizeof(YCMDstr), "SMS: %s. Avtori: %s[%d]",params[1],Name(playerid), playerid);
			SendClientMessage(i, 0xFDE640AA, YCMDstr);
			PlayerPlaySound(i, 1052, 0.0, 0.0, 0.0);
			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
			format(YCMDstr, sizeof(YCMDstr), "SMS: %s. Mimgebi: %s[%d]",params[1],Name(i), i);
			SendClientMessage(playerid,  0xFDE640AA, YCMDstr);
			SendClientMessage(playerid,  COLOR_WHITE, "- SMS Migebulia");
			format(YCMDstr, sizeof(YCMDstr), "%s'm(a) Gaagzavna SMS",Name(playerid));
			return true;
		}
		else if(IsPlayerConnected(params[0]))
		{
			if(PhoneOnline[params[0]] > 0)
			{
				SendClientMessage(playerid, COLOR_GREY, "[Operatori]: Mobiluri telefoni gamortulia an gasulia momsaxureobis zonidan");
				return true;
			}
			if(PlayerInfo[playerid][pMobile] < 1) return SendClientMessage(playerid, COLOR_GREY, "[Operatori]: Tqvens angarishze tanxa araris");
			format(YCMDstr, sizeof(YCMDstr), "SMS: %s. Gamomgzavni: %s[%d]",params[1],Name(playerid), playerid);
			SendClientMessage(params[0], COLOR_YELLOW, YCMDstr);
			PlayerPlaySound(params[0], 1052, 0.0, 0.0, 0.0);
			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
			format(YCMDstr, sizeof(YCMDstr), "%s'm(a) Gaagzavna SMS",Name(playerid));
			ProxDetector(5.0, playerid, YCMDstr, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
			format(YCMDstr, sizeof(YCMDstr), "SMS: %s. Mimgebi: %s[%d]",params[1],Name(params[0]),params[0]);
			SendClientMessage(playerid,  COLOR_YELLOW, YCMDstr);
			SendClientMessage(playerid,  COLOR_WHITE, "SMS Migebulia");
			PlayerInfo[playerid][pMobile] -= 20;
			foreach(new a:Player)
			{
		  		if(adminreadsms[a] == 1 && PlayerInfo[a][pAdmin] > 5)
				{
				        new string5[128];
			            format(string5,sizeof(string5),"ID %d For %d | Text: %s",playerid,params[0],params[1]);
			            SendClientMessage(a,0xFF0000AA,string5);
		    	}
			}
			return true;
		}
	}
	return true;
}
