publics: kick(giveplayerid)
{
	if(PlayerInfo[giveplayerid][pLogin] == 1) OnPlayerUpdateAc(giveplayerid);
	if(caridhouse[giveplayerid] != -1)
	{

		DestroyVehicle(caridhouse[giveplayerid]);
		
		if(GetPVarInt(giveplayerid, "fixcarn") == 1)
		{
			PlayerInfo[giveplayerid][pFuelcar] = Fuell[caridhouse[giveplayerid]];
		}
		else if(GetPVarInt(giveplayerid, "fixcarn") == 2)
		{
			PlayerInfo[giveplayerid][pFuelcar1] = Fuell[caridhouse[giveplayerid]];
		}
		else if(GetPVarInt(giveplayerid, "fixcarn") == 3)
		{
			PlayerInfo[giveplayerid][pFuelcar2] = Fuell[caridhouse[giveplayerid]];
		}
		
		caridhouse[giveplayerid] = -1;
		createdcar --;
	}
	Kick(giveplayerid);
	return true;
}
