		{
            if(PlayerInfo[i][pLeader] == teamnumber) format(string, sizeof(string), "%s[%d] Rang: Leader Warns: %d/3",Name(i), i, PlayerInfo[i][pVig], chislo++);
		    else if(PlayerInfo[i][pJobHeal] == PlayerInfo[playerid][pJobHeal] && FormaFrac[i] > 0) format(string, sizeof(string), "%s[%d] Rang: %d",Name(i), i, PlayerInfo[i][pRank]), chislo++;
		}
