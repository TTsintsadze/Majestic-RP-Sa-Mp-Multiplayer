		    if(stolid != GetPVarInt(playerid,"StolReg")) return SendClientMessage(playerid,COLOR_GREY,"Tqven Ar Gagiketebiat Fsoni");
		    if(StolInfo[stolid][GameStart] == 0) return SendClientMessage(playerid,COLOR_GREY,"Am Momentshi Ver Gaagorebt Qvebs");
