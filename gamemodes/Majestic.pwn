CMD:time(playerid,params[])
{
	if(IsPlayerInRangeOfPoint(playerid, 80.0, 1962.7936,1018.1185,992.4688)) return SendClientMessage(playerid, COLOR_WHITE, "{FF0000}[Error] {FFFFFF}Cazinoshi Am CMD's Gamoyeneba Akrdzalulia!");
	if(PlayerInfo[playerid][pLogin] == 0) return 1;
	if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
    {
       	ApplyAnimation(playerid, "COP_AMBIENT", "Coplook_watch",4.1,0,0,0,0,0);
	}
	new string[64], mtext[20], year, month,day;
	getdate(year, month, day);
	format(string, sizeof(string), "%s'm(a) daxeda saats", Name(playerid));
	SetPlayerChatBubble(playerid, string, 0xDD90FFFF, 10.0, 5000);
	switch(month)
	{
	    case 1:mtext = "January";
	    case 2: mtext = "February";
	    case 3: mtext = "March";
	    case 4: mtext = "April";
	    case 5:mtext = "Maya";
	    case 6: mtext = "June";
	    case 7: mtext = "July";
	    case 8: mtext = "August";
	    case 9: mtext = "Sentebrya";
	    case 10:mtext = "October";
	    case 11: mtext = "November";
	    case 12: mtext = "December";
	}
	new hour,minuite,second;
	gettime(hour,minuite,second);
	if (minuite < 10)
	{
		if(PlayerInfo[playerid][pJailTime] > 0)format(string, sizeof(string), "~y~%d ~y~%s~n~~w~%d:0%d~n~ ~g~ concluded ~w~%d", day, mtext, hour, minuite, PlayerInfo[playerid][pJailTime]-10);
		else format(string, sizeof(string), "~y~%d ~y~%s~n~~w~%d:0%d", day, mtext, hour, minuite);
	}
	else
	{
		if(PlayerInfo[playerid][pJailTime] > 0) format(string, sizeof(string), "~y~%d ~y~%s~n~~w~%d:%d~n~ ~g~ concluded ~w~%d", day, mtext, hour, minuite, PlayerInfo[playerid][pJailTime]-10);
		else format(string, sizeof(string), "~y~%d ~y~%s~n~~w~%d:%d", day, mtext, hour, minuite);
	}
	GameTextForPlayer(playerid, string, 5000, 1);
	return 1;
}
