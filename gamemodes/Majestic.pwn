			LSNews(0x00BFFFAA, string);
		//	// Flood[playerid] = 2;
		}
		return false;
	}
    if(realchat)
	{
		if(PlayerInfo[playerid][pLogin] == 0)
		{
			return false;
		}
		if(PlayerInfo[playerid][pMuted] == 1)
