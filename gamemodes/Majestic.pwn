			SendClientMessage(playerid, COLOR_LIGHTRED, string);
			return false;
		}
		if(PlayerInfo[playerid][pLevel] <= 1)
		{
			new succ;
			for(new i; i<strlen(text); i++) if(PText[playerid][i] == text[i]) succ++;
			if(succ == strlen(text))
			{
				SendClientMessage(playerid,0xFFD5BBAA, "Nu Floodav");
				return false;
			}
		}
		strmid(PText[playerid],text,0,256,256);
		if(animon[playerid] == false && Tazed[playerid] == false)
		{
			ApplyAnimation(playerid, "PED", "IDLE_CHAT",4.1,0,1,1,1,1,1);
			SetTimerEx("ClearAnim", 2000, false, "d", playerid);
		}
