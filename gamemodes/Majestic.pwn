	}
	ini_closeFile(File);
}
publics: GzCheck()
{
	for(new i = 0; i < sizeof(GZInfo); i++)
	{
		if(ZoneOnBattle[i] == 1)
		{
			if(GZSafeTime[i] > 0) GZSafeTime[i] --;
            foreach(new x:Player)
 			{
				new y = PlayerInfo[x][pMember];
				if(y == GZInfo[i][gNapad] || y == GZInfo[i][gFrakVlad])
				{
					if(CaptureOn[x] == 0)
					{
					    switch(GZInfo[i][gNapad])
						{
							case 12:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][0], -16711681);
							}
							case 13:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][0], -65281);
							}
							case 15:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][0], 8388863);
							}
							case 17:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][0], 16777215);
							}
							case 18:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][0], 8454143);
							}
						}
						switch(GZInfo[i][gFrakVlad])
						{
							case 12:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][1], -16711681);
							}
							case 13:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][1], -65281);
							}
							case 15:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][1], 8388863);
							}
							case 17:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][1], 16777215);
							}
							case 18:
							{
								PlayerTextDrawColor(x,CaptTXT_P[x][1], 8454143);
				            }
				        }
				        for(new t; t != 11; t++) TextDrawShowForPlayer(x, CaptTXT[t]);
				        for(new p; p != 7; p++) PlayerTextDrawShow(x, CaptTXT_P[x][p]);
						CaptureOn[x] = 1;
					}
					if(GZSafeTime[i] > 0) format(capt_str,sizeof(capt_str),"%s",Convert(GZSafeTime[i]));
					PlayerTextDrawSetString(x, CaptTXT_P[x][4], capt_str);

					format(capt_str,sizeof(capt_str),"%d", CountOnZone[GZInfo[i][gFrakVlad]]);
					PlayerTextDrawSetString(x, CaptTXT_P[x][3], capt_str);
					format(capt_str,sizeof(capt_str),"%d", CountOnZone[GZInfo[i][gNapad]]);
					PlayerTextDrawSetString(x, CaptTXT_P[x][2], capt_str);

					format(capt_str,sizeof(capt_str),"%d", CaptKill[x]);
					PlayerTextDrawSetString(x, CaptTXT_P[x][5], capt_str);
					format(capt_str,sizeof(capt_str),"%d", CaptDeath[x]);
					PlayerTextDrawSetString(x, CaptTXT_P[x][6], capt_str);
				}
			}
			if(GZSafeTime[i] <= 0)
			{
			    new highscore = -1;
				new highscoreowner = INVALID_PLAYER_ID;
				foreach(new p:Player)
				{
				    new g = PlayerInfo[p][pMember];
					if(g == GZInfo[i][gNapad] || g == GZInfo[i][gFrakVlad])
					{
						if(KillCounter[p] > highscore)
						{
							highscore = KillCounter[p];
							highscoreowner = p;
						}
					}
				}
				if(CountOnZone[GZInfo[i][gFrakVlad]] > CountOnZone[GZInfo[i][gNapad]])
				{
				    foreach(new x:Player)
				    {
				        new y = PlayerInfo[x][pMember];
						if(y == GZInfo[i][gNapad] || y == GZInfo[i][gFrakVlad])
						{
					        for(new t; t != 11; t++) TextDrawHideForPlayer(x, CaptTXT[t]);
					        for(new p; p != 7; p++) PlayerTextDrawHide(x, CaptTXT_P[x][p]);
							KillCounter[x] = 0;
							CaptureOn[x] = 0;
