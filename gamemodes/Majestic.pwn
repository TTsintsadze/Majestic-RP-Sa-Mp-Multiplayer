				SetPlayerWantedLevel(i, 0);
				SetPlayerVirtualWorld(i, 0);
				GameTextForPlayer(i,"~g~Svabodaa", 5000, 1);
				OnPlayerUpdateAc(i);
				SpawnPlayer(i);
			}
		}
		if(PlayerTazeTime[i] >= 1) PlayerTazeTime[i] += 1;
		if(PlayerTazeTime[i] >= 15) PlayerTazeTime[i] = 0, WC_TogglePlayerControllable(i, 1);
		if(TieTime[i] > 0) TieTime[i]--;
		if(PlayerCuffed[i] == 1)
		{
			if(PlayerCuffedTime[i] <= 0)
			{
				WC_TogglePlayerControllable(i, 1);
				PlayerCuffed[i] = 0;
				PlayerCuffedTime[i] = 0;
				PlayerTazeTime[i] = 1;
			}
			else PlayerCuffedTime[i] -= 1;
		}
		if(PlayerCuffed[i] == 2)
		{
			if(PlayerCuffedTime[i] <= 0)
			{
				WC_TogglePlayerControllable(i, 1);
				PlayerCuffed[i] = 0;
				PlayerCuffedTime[i] = 0;
			}
			else PlayerCuffedTime[i] -= 1;
		}
	}
	if ((hour > ghour) || (hour == 0 && ghour == 23))
	{
