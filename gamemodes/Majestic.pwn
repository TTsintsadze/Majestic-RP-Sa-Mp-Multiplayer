publics: LoadMZ()
{
	new string[50];
	new mfilea = ini_openFile("gamefiles/mafiazone.ini");
	new l[2000];
	new asd[5][10];
	for(new i =0;i<sizeof(McInfo);i++)
	{
		format(string,sizeof(string), "Zone%d",i);
		new x = ini_getString(mfilea,string,l);
		if(x < 0)
		{
			McInfo[i][Coords][0] = STANDARTGangZone[i][0];
			McInfo[i][Coords][1] = STANDARTGangZone[i][1];
			McInfo[i][Coords][2] = STANDARTGangZone[i][2];
			McInfo[i][Coords][3] = STANDARTGangZone[i][3];
		}
		else
		{
			split(l,asd,'|');
			McInfo[i][Coords][0] = floatstr(asd[0]);
			McInfo[i][Coords][1] = floatstr(asd[1]);
			McInfo[i][Coords][2] = floatstr(asd[2]);
			McInfo[i][Coords][3] = floatstr(asd[3]);
		}
		McInfo[i][FrakVlad] = strval(asd[4]);
		McInfo[i][ID] = GangZoneCreate(McInfo[i][Coords][0],McInfo[i][Coords][1],McInfo[i][Coords][2],McInfo[i][Coords][3]);
		ZoneOnMBattle[i] = 0;
		MSafeTime[i] = 0;
	}
	ini_closeFile(mfilea);
}
publics: SaveMZ()
{
	new string[400];
	new mfilea = ini_openFile("gamefiles/mafiazone.ini");
	new l[10];
	for(new i =0;i<sizeof(McInfo);i++)
	{
		format(l,sizeof(l), "Zone%d",i);
		format(string,sizeof(string), "%f|%f|%f|%f|%d",McInfo[i][Coords][0], McInfo[i][Coords][1],McInfo[i][Coords][2],McInfo[i][Coords][3],McInfo[i][FrakVlad]);
		ini_setString(mfilea,l,string);
	}
	ini_closeFile(mfilea);
}
publics: MfCheck()
{
	for(new i = 0; i < sizeof(McInfo); i++)
	{
		if(ZoneOnMBattle[i] == 1)
		{
			if(MSafeTime[i] > 0) MSafeTime[i] --;
            foreach(new x:Player)
 			{
				new string[128];
				new y = PlayerInfo[x][pMember];
				if(y == McInfo[i][Napad] || y == McInfo[i][FrakVlad])
				{
					if(MFCaptOn[x] == 0)
					{
						PlayerTextDrawShow(x, Capture[x]);
						MFCaptOn[x] = 1;
					}
					if(MSafeTime[i] > 0) format(capt_str,sizeof(capt_str),"~w~ %s",Convert(MSafeTime[i]));
					format(string,sizeof(string),"~y~%s~n~~g~%s:~w~ %i~n~~r~%s:~w~ %i",capt_str,GetMafiaName(McInfo[i][Napad]),CountOnMFZone[McInfo[i][Napad]],GetMafiaName(McInfo[i][FrakVlad]),CountOnMFZone[McInfo[i][FrakVlad]]);
					PlayerTextDrawSetString(x,Capture[x],string);
				}
			}
			if(MSafeTime[i] <= 0)
			{
				if(CountOnMFZone[McInfo[i][FrakVlad]] > CountOnMFZone[McInfo[i][Napad]])
				{
				    foreach(new x:Player)
				    {
				        new y = PlayerInfo[x][pMember];
						if(y == McInfo[i][Napad] || y == McInfo[i][FrakVlad])
						{
							MFCaptOn[x] = 0;
                            PlayerTextDrawHide(x, Capture[x]);
						}
					}
					SendFamilyMessage(McInfo[i][FrakVlad],0x00B953AA,"{FFFFFF}Gilocavt, Tqven Sheinarchunet Tqveni Teritoria");
					SendFamilyMessage(McInfo[i][Napad],0xC56565AA,"{FF0000}Samwuxarod Tqven Ver Daipyarit Axali Teritoria");
					CountOnMFZone[McInfo[i][FrakVlad]] = 0;
					CountOnMFZone[McInfo[i][Napad]] = 0;
				}
				else if(CountOnMFZone[McInfo[i][FrakVlad]] < CountOnMFZone[McInfo[i][Napad]])
				{
				    foreach(new x:Player)
				    {
				        new y = PlayerInfo[x][pMember];
						if(y == McInfo[i][Napad] || y == McInfo[i][FrakVlad])
						{
							MFCaptOn[x] = 0;
							PlayerTextDrawHide(x, Capture[x]);
						}
					}
					CountOnMFZone[McInfo[i][FrakVlad]] = 0;
					CountOnMFZone[McInfo[i][Napad]] = 0;
					SendFamilyMessage(McInfo[i][Napad], 0x00B953AA,"{FFFFFF}Gilocavt, Tqven Sheinarchunet Tqveni Teritoria");
					SendFamilyMessage(McInfo[i][FrakVlad], 0xC56565AA,"{FF0000}Samwuxarod Tqven Dakarget Teritoriaze Kontroli.");
					McInfo[i][FrakVlad] = McInfo[i][Napad];
					McInfo[i][Napad] = 0;
				}
				else if(CountOnMFZone[McInfo[i][FrakVlad]] == CountOnMFZone[McInfo[i][Napad]])
				{

				    foreach(new x:Player)
				    {
				        new y = PlayerInfo[x][pMember];
						if(y == McInfo[i][Napad] || y == McInfo[i][FrakVlad])
						{
							MFCaptOn[x] = 0;
							PlayerTextDrawHide(x, Capture[x]);
						}
					}
					CountOnMFZone[McInfo[i][FrakVlad]] = 0;
					CountOnMFZone[McInfo[i][Napad]] = 0;
					SendFamilyMessage(McInfo[i][Napad], 0x00B953AA,"{FFFFFF}Gilocavt, Tqven Sheinarchunet Tqveni Teritoria");
					SendFamilyMessage(McInfo[i][FrakVlad], 0xC56565AA,"{FF0000}Samwuxarod Tqven Dakarget Teritoriaze Kontroli.");
					McInfo[i][FrakVlad] = McInfo[i][Napad];
					McInfo[i][Napad] = 0;
				}
				ZoneOnMBattle[i] = 0;
				GangZoneStopFlashForAll(McInfo[i][ID]);
				GangZoneHideForAll(McInfo[i][ID]);
				SaveMZ();
				GangZoneShowForAll(McInfo[i][ID],GetMafiaZoneColor(i));
				if(captures == 0) KillTimer(MfCheckTimer), MfCheckOn = 0;
				break;
			}
		}
	}
	return true;
}
stock IsInAllowed(playerid)
{
	for(new i = 0; i < sizeof(allowedfactions); i++)
	{
		if(PlayerInfo[playerid][pMember] == allowedfactions[i]&&PlayerInfo[playerid][pRank] >= 7) return true;
	}
	return false;
}
stock IsInAllowedM(mnumbers)
{
	for(new i =0;i<sizeof(allowedmfactions);i++)
	{
		if(mnumbers == allowedmfactions[i]) return true;
	}
	return false;
}
stock GetXYInFrontOfPlayer(playerid, &Float:q, &Float:w, Float:distance)
{
    new Float:a;
    GetPlayerPos(playerid, q, w, a);
    GetPlayerFacingAngle(playerid, a);
    q += (distance * floatsin(-a, degrees));
    w += (distance * floatcos(-a, degrees));
    return 1;
}
