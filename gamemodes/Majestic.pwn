		return false;
	}
	if(strcmp(text, ":D", true) == 0|| strcmp(text, ":d", true) == 0 || strcmp(text, "ddd", true) == 0)
	{
		if(PlayerInfo[playerid][pMuted] == 1) { SendClientMessage(playerid, COLOR_LIGHTRED, "Tqven Gadevt Chatis Bani"); return false; }
