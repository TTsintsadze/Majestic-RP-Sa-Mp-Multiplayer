public OnPlayerDisconnect(playerid, reason)
{
	if(PlayerInfo[playerid][pBeg][1])
    {
        PlayerInfo[playerid][pBeg][1] = 0;
        DestroyDynamicObject(PlayerInfo[playerid][pBeg][2]);
        DestroyDynamicObject(PlayerInfo[playerid][pBeg][2]+1);
    }
	// VOICE SYSTEM //
	if (lstream[playerid])
	{
		SvDeleteStream(lstream[playerid]);
		lstream[playerid] = SV_NULL;
	}
	// VOICE SYSTEM //
	new string[100];
	if(KostiName[playerid] != INVALID_PLAYER_ID)
	{
		SendClientMessage(KostiName[playerid],-1,"Kamatlebi: Tqvenma Mowinaagvmdegem Datova Serveri");
  		KostiName[KostiName[playerid]] = INVALID_PLAYER_ID;
		KostiName[playerid]  = INVALID_PLAYER_ID;
	}
    if(PlayerInfo[playerid][pCar][0] == 508 && caridhouse[playerid] != 0)
	{
		new Float:XX, Float:YY, Float:ZZ,Float:CC;
		GetVehiclePos(caridhouse[playerid], XX, YY, ZZ);
		GetVehicleZAngle(caridhouse[playerid], CC);
		if(GetPVarInt(playerid, "Intropic" ) == caridhouse[playerid])
		{
			new Float:X, Float:Y, Float:Z;
			GetVehiclePos(GetPVarInt( playerid, "Intropic" ), X, Y, Z);
			SetPlayerPos(playerid, X, Y, Z);
			SetPlayerVirtualWorld(playerid,0);
			SetPlayerInterior(playerid, 0);
			DeletePVar(playerid, "Intropic");
		}
		OnPlayerUpdateAc(playerid);
		DestroyVehicle(caridhouse[playerid]);
		caridhouse[playerid] = 0;
	}
	if(caridhouse[playerid] != -1)
	{
	    DestroyVehicle(caridhouse[playerid]);
		if(GetPVarInt(playerid, "fixcarn") == 1)
		{
			PlayerInfo[playerid][pFuelcar] = Fuell[caridhouse[playerid]];
		}
		else if(GetPVarInt(playerid, "fixcarn") == 2)
		{
			PlayerInfo[playerid][pFuelcar1] = Fuell[caridhouse[playerid]];
		}
		else if(GetPVarInt(playerid, "fixcarn") == 3)
		{
			PlayerInfo[playerid][pFuelcar2] = Fuell[caridhouse[playerid]];
		}
		caridhouse[playerid] = -1;
		createdcar --;
	}
    KillTimer(chantis[playerid]);
    KillTimer(sborit[playerid]);
    //new idx = cvector_find(players,playerid);
    //if(idx !=-1) cvector_remove(players,idx);
    foreach(new i:Player)
	{
    	if(GetPlayerState(i) == PLAYER_STATE_SPECTATING && gSpectateID[i] == playerid)
    	{
        	StopSpectate(i);
       	 	SetPlayerHealth(i, 100);
        	GameTextForPlayer(i,"~w~RECON ~r~ OFF~n~ ~r~PLAYER DISCONNECT", 5000, 3);
    	}
	}
	if(PlayerRacing[playerid] == 1)
	{
		PlayerRacing[playerid] = 0;
		RacingPlayers -= 1;
	}
	if(Tazed[playerid] == true)
	{
	    PlayerInfo[playerid][pJailed] = 1;
		PlayerInfo[playerid][pWantedLevel] = 0;
		PlayerInfo[playerid][pJailTime] = 1000;
		PlayerInfo[playerid][pArrested] += 1;
		PlayerInfo[playerid][pMestoJail] = 1;
	}
	if(PlayerCuffed[playerid] > 0)
	{
	    PlayerInfo[playerid][pJailed] = 1;
		PlayerInfo[playerid][pWantedLevel] = 0;
		PlayerInfo[playerid][pJailTime] = 600;
		PlayerInfo[playerid][pArrested] += 1;
		PlayerInfo[playerid][pMestoJail] = 1;
	}
	if(creatcar[playerid] != -1)
	{
	 	DestroyVehicle(creatcar[playerid]);
 		creatcar[playerid] = -1;
	}
	if(skustom[playerid] == true) skustom[playerid] = false, RemovePlayerAttachedObject(playerid,3);
	SetPVarInt(playerid,"Register",0);
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && TransportDuty[playerid] == 1)
    {
        Delete3DTextLabel(taxi3d[GetPlayerVehicleID(playerid)]);
		TransportDuty[playerid] = 0;
	}
    new stringr[180];
	format(stringr, sizeof(stringr), "UPDATE accounts SET pLogin = '0' WHERE name = '%s'",Name(playerid));
	mysql_tquery(connects, stringr, "", "");
	new stolid = GetPVarInt(playerid,"GameStol");
	if(stolid != 88 )
	{
	    new g = KazinoDuty[playerid];
		TextDrawHideForPlayer(playerid,KostiBox[stolid]);
   		TextDrawHideForPlayer(playerid,TableName[stolid]);
   		TextDrawHideForPlayer(playerid,KostiInfo[stolid]);
   		TextDrawHideForPlayer(playerid,KostiGameInfo[stolid]);
   		TextDrawHideForPlayer(playerid,KostiKeyExit);
   		TextDrawHideForPlayer(playerid,KostiKeyDone);
   		TextDrawHideForPlayer(playerid,KostiKeyStavka);
   		SetPVarInt(playerid,"GameStol",88);
   		SetPVarInt(playerid,"StolReg",88);
   		WC_TogglePlayerControllable(playerid,1);
   		CancelSelectTextDraw(playerid);
   		DelName(playerid,stolid);
   		SetPVarInt(playerid,"Ochki",88);
     	if(g > 0) SetTimerEx("NullStol",30000, false, "i", stolid), StolInfo[stolid][Stavka] = 0;
	}
	if(caridhouse[playerid] != -1)
	{
		PlayerInfo[playerid][pProcents][0] = PlayerInfo[playerid][pProcents][0];
		PlayerInfo[playerid][pFuelcar] = Fuell[caridhouse[playerid]];
		caridhouse[playerid] = -1;
		createdcar --;
	}
	if(STimer[playerid] != -1)
	{
      	KillTimer(STimer[playerid]);
      	STimer[playerid] = -1;
	}
	for(new i = 0; i < 7; i++)
	{
		PlayerTextDrawDestroy(playerid, speeddraw[playerid][i]);
		speeddraw[playerid][i] = PlayerText:INVALID_TEXT_DRAW;
	}
	//
	format(AdminNick[playerid],MAX_PLAYER_NAME, "");
	PlayerInfo[playerid][pForma] = FormaFrac[playerid];
	if(PlayerInfo[playerid][pJob] == 2 && gcontract[playerid] == 1) Delete3DTextLabel(Meh3d[GetPlayerVehicleID(playerid)]), gcontract[playerid] = 0;
	new sendername[MAX_PLAYER_NAME];
	new year, month,day;
	FormaFrac[playerid] = 0;
	format(WantNickChange[playerid],MAX_PLAYER_NAME, "");
	getdate(year, month, day);
	new hour, minute, second;
	gettime(hour, minute, second);
	GetPlayerName(playerid, sendername, sizeof(sendername));
	if(IsPlayerAttachedObjectSlotUsed(playerid,1)) RemovePlayerAttachedObject(playerid,1);
	soglasen[playerid] = 0;
	CaptDeath[playerid] = 0;
	CaptKill[playerid] = 0;
	guncheat[playerid] = 0;
	SelectCharPlace[playerid] = 0;
