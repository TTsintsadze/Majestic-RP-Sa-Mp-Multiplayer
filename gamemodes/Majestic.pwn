		//
		//
	/*	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		{
			Fuell[GetPlayerVehicleID(playerid)] = 200;
		}*/
		new weaponid = GetPlayerWeapon(playerid);
		if(weaponid == 16 || weaponid == 18 || weaponid == 4 || weaponid == 9 || weaponid == 45 || weaponid == 44
		|| weaponid == 37 || weaponid == 39 || weaponid == 42 || weaponid == 36 || weaponid == 34 || weaponid == 35 || weaponid == 27)
		{
		   // ResetWeapons(playerid);
			Kick(playerid);
		}
		if(weaponid == 38 || weaponid == 18 || weaponid == 28 || weaponid == 26
		|| weaponid == 27 || weaponid == 35 || weaponid == 36 || weaponid == 37	|| weaponid == 32
		|| weaponid == 39 || weaponid == 40 || weaponid == 9  || weaponid == 42  || weaponid == 38)
		{
		  //  ResetWeapons(playerid);
		  	if(PlayerInfo[playerid][pAdmin] != 0) return true;
	        SendClientMessage(playerid, COLOR_LIGHTRED, "Tqven Gaikicket Cheterobistvis (#010)");
			Kick(playerid);
		}
		//
		new Float:VHP;
		GetVehicleHealth(GetPlayerVehicleID(playerid),VHP);
		if(VHP >= 1000 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		{
	        //SendClientMessage(playerid,COLOR_LIGHTRED,"Tqven Gaickiket Cheterobis Gamo! (#011)");
            if(PlayerInfo[playerid][pAdmin] < 1){ Kick(playerid); }
            SetVehicleHealth(GetPlayerVehicleID(playerid),999.0);
		}
