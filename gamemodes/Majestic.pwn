forward UpdateSpeedometr(playerid);
public UpdateSpeedometr(playerid)
{
	new vehicleid = GetPlayerVehicleID(playerid), str[16];
	new Float:vhealth;
	GetVehicleHealth(vehicleid, vhealth);
    format(str, sizeof(str), "%d", SpeedVehicle(playerid));
	PlayerTextDrawSetString(playerid,speeddraw[playerid][4],str);
	format(str, sizeof(str), "fuel: %.0f%", Fuell[vehicleid]);
	PlayerTextDrawSetString(playerid,speeddraw[playerid][5],str);
	format(str, sizeof(str), "%.0f%", vhealth);
	PlayerTextDrawSetString(playerid,speeddraw[playerid][6],str);

	if(IsLocked[vehicleid])
	{
		PlayerTextDrawBoxColor(playerid, speeddraw[playerid][0], -1407312129);
		PlayerTextDrawShow(playerid, speeddraw[playerid][0]);
	}
	else
	{
		PlayerTextDrawBoxColor(playerid, speeddraw[playerid][0], 576000767);
		PlayerTextDrawShow(playerid, speeddraw[playerid][0]);
	}
	if(Light[vehicleid])
	{
		PlayerTextDrawBoxColor(playerid, speeddraw[playerid][1], -949019137);
		PlayerTextDrawShow(playerid,speeddraw[playerid][1]);
	}
	else
	{
        PlayerTextDrawBoxColor(playerid, speeddraw[playerid][1], 0x848482FF);
		PlayerTextDrawShow(playerid,speeddraw[playerid][1]);
	}
	if(zavodis[vehicleid])
	{
		PlayerTextDrawBoxColor(playerid, speeddraw[playerid][2], 576000767);
		PlayerTextDrawShow(playerid,speeddraw[playerid][2]);
	}
	else
	{
		PlayerTextDrawBoxColor(playerid, speeddraw[playerid][2], -1407312129);
		PlayerTextDrawShow(playerid,speeddraw[playerid][2]);
	}
	new temptext[256];
	if(SpeedVehicle(playerid) > 0)
	{
	    format(temptext, sizeof(temptext), "");
		for(new dspeed; dspeed < floatround((10.0/130.0)*float(SpeedVehicle(playerid))) && dspeed < 13; dspeed++)
		{
		    switch(dspeed)
		    {
		        case 7:format(temptext, sizeof(temptext), "%s~g~", temptext);
		        case 11:format(temptext, sizeof(temptext), "%s~r~", temptext);
		    }
		    format(temptext, sizeof(temptext), "%sI", temptext);
		}
		PlayerTextDrawSetString(playerid, speeddraw[playerid][3], temptext);
	}
	else
	{
		PlayerTextDrawSetString(playerid, speeddraw[playerid][3], "_");
	}
	if(PlayerInfo[playerid][pJob] == 1 && vehicleid >= Sweeper[0] && vehicleid <= Sweeper[7])
	{
		new Float:sx, Float:sy, Float:sz;
		GetPlayerPos(playerid, sx,sy,sz);
		new mandzili = floatround(GetDistanceBetweenPoints(JCOORD[playerid][0],JCOORD[playerid][1],JCOORD[playerid][2], sx,sy,sz), floatround_round);
		if(mandzili > 100)
		{
		    Kilometrebi[playerid]++;
		    if(PlayerInfo[playerid][pQuest][0] == 4) PlayerInfo[playerid][pQuest][1]++;
			GetPlayerPos(playerid, JCOORD[playerid][0],JCOORD[playerid][1],JCOORD[playerid][2]);
		}
		format(YCMDstr, sizeof(YCMDstr), "~w~~n~~n~~n~~n~~n~~n~~n~Gawmendilia %d KM", Kilometrebi[playerid]);
		GameTextForPlayer(playerid,YCMDstr,500,3);
	}
	return 1;
}
