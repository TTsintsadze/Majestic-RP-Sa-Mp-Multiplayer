{
    for(new s;s<MAX_COSTI;s++)
   	{
    	new str[256],sstr[40];
    	format(str,sizeof(str),"Fsoni: %d ~g~$~n~~w~Bank: %d ~g~$~n~",StolInfo[s][Stavka],StolInfo[s][Bank]);
