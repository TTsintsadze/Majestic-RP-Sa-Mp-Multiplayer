		}
		if (realtime) SetWorldTime(hour);
	}
	if(minute == 30 && second == 05)
	{
		PayDay();
	}
	if(hour == 04 && minute == 50 && second == 00)
	{
	    for(new i = 1; i <= TotalBizz; i++) SaveBizz(i);
    	for(new i = 1; i <= TotalHouse; i++) SaveProperty(i);
		SaveAccounts();
		SaveMaterials();
		SaveGZ();
		SaveMZ();
		SaveBankFrac();
	}
	if(hour == 00 && minute == 00 && second == 00)
	{
		mysql_query(connects,"UPDATE `accounts` SET pChas = '0',pMin = '0',pOnlineLid = '00:00'");
		foreach(new i:Player)
		{
			PlayerInfo[i][pChas] = 0;
			PlayerInfo[i][pMin] = 0;
			strmid(PlayerInfo[i][pOnlineLid],"00:00", 0, strlen("00:00"), 10);
		}
	}
	if(hour == 12 && minute == 10  || hour == 18 && minute == 10  || hour == 21 && minute == 10  || hour == 15 && minute == 10 || hour == 23 && minute == 10 )
	{
		if(EndingRaceRound == 1)
		{
			format(string, sizeof(string), "Yuradgeba! Rbola Iwyeba 5 Wutshi. Trasa: Los Santos Aeroport");
			//SendClientMessageToAll(0xB9B900AA,string);
			EndingRaceRound = 0;
			SetTimer("StartRacetwo", 180000, 0);
		}
	}
	UpdateCasino();
	return true;
}
