CMD:bizlock(playerid)
{
    if(PlayerInfo[playerid][pLogin] == 0) return 1;
	for(new i = 1; i <= TotalBizz; i++)
	{
		if(PlayerToPoint(3, playerid,BizzInfo[i][bEntranceX], BizzInfo[i][bEntranceY], BizzInfo[i][bEntranceZ]) || !PlayerToPoint(3, playerid,BizzInfo[i][bExitX], BizzInfo[i][bExitY], BizzInfo[i][bExitZ]))
		{
			if(PlayerInfo[playerid][pMember] == 2 && PlayerInfo[playerid][pRank] >= 5)
			{
				if(BizzInfo[i][bLocked] == 1)
				{
					BizzInfo[i][bLocked] = 0;
					SendClientMessage(playerid, COLOR_GREY, "Tqven gaxsenit biznesi");
					SaveBizz(i);
					return 1;
				}
				if(BizzInfo[i][bLocked] == 0)
				{
					BizzInfo[i][bLocked] = 1;
					SendClientMessage(playerid, COLOR_GREY, "Tqven daxuret biznesi");
					SaveBizz(i);
					return 1;
				}
			}
			else if(PlayerInfo[playerid][pPbiskey] == i)
			{
				if(BizzInfo[i][bLocked] == 1)
				{
					BizzInfo[i][bLocked] = 0;
					BizzInfo[i][bLockTime] = 0;
					SendClientMessage(playerid, COLOR_GREY, "Tqven gaxsenit biznesi");
					SaveBizz(i);
					return 1;
				}
				if(BizzInfo[i][bLocked] == 0)
				{
					if(BizzInfo[i][bProducts] <= 0) return SendClientMessage(playerid, COLOR_GREY, "Tqven ver gaxsnit biznes radgan araa produqti.");
					BizzInfo[i][bLocked] = 1;
					SendClientMessage(playerid, COLOR_GREY, "Tqven daxuret biznesi");
					SaveBizz(i);
					return 1;
				}
			}
		}
	}
	return 1;
}
