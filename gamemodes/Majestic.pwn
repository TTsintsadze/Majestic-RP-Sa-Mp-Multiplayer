		return false;
	}
	if(PEfir[playerid] != 255)
	{
		if(PlayerInfo[playerid][pMember] == 16 || PlayerInfo[playerid][pLeader] == 16)
		{
			if(PlayerInfo[playerid][pMuted] == 1) {SendErrorMessage(playerid, "Tkven Gadevt Chatis Bani!"); return false; }
