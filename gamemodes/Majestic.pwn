		{
			ResetPlayerMoney(playerid);
			GivePlayerMoney(playerid, PlayerInfo[playerid][pCash]);
		}
		//
		if(PlayerInfo[playerid][pPhousekey] != 999 && PlayerInfo[playerid][pQira] != 999)
		{
		    PlayerInfo[playerid][pQira] = 999;
		    OnPlayerUpdateAc(playerid);
		}
		new target = GetPlayerTargetPlayer(playerid);
		if(target != INVALID_PLAYER_ID && PlayerInfo[target][pWantedLevel] > 0 && PlayerInfo[playerid][pMember] == 1 && GetPlayerSpecialAction(target) != SPECIAL_ACTION_HANDSUP)
		{
		    new Float:a;
		    new Float:s;
		    new Float:d;
		    GetPlayerPos(target, a,s,d);
		    if(!PlayerToPoint(30.0,playerid,a,s,d)) return true;
  			new gun = GetPlayerWeapon(playerid);
  			new gun1 = GetPlayerWeapon(target);
		    if(gun == 22 || gun == 23 || gun == 24 || gun == 25 || gun == 29 || gun == 30 || gun == 31)
		    {
		        //if(IsAGang(playerid) && IsAGang(target)) return true;
		        if(gun1 == 22 || gun1 == 23 || gun1 == 24 || gun1 == 25 || gun1 == 29 || gun1 == 30 || gun1 == 31) return true;
		    	SetPlayerSpecialAction(target, SPECIAL_ACTION_HANDSUP);
			}
		}
		//============== gotome ============================//
		if(gotome[playerid] != -1)
		{
		    new Float:x, Float:y, Float:z;
		    GetPlayerPos(gotome[playerid], x, y, z);
		    SetPlayerPos(playerid, x, y+1, z);
			SetPlayerInterior(playerid,GetPlayerInterior(gotome[playerid]));
			SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(gotome[playerid]));
		}
		if(PlayerInfo[playerid][pChar][PlayerInfo[playerid][pViborChar]] == 74 && !Mtavari(playerid))
		{
		    PlayerInfo[playerid][pChar][PlayerInfo[playerid][pViborChar]] = 23;
		    SetPlayerSkin(playerid, PlayerInfo[playerid][pChar][PlayerInfo[playerid][pViborChar]]);
		}
		SetPlayerWantedLevel(playerid, PlayerInfo[playerid][pWantedLevel]);
		return true;
}
