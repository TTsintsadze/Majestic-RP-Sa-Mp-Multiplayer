			//// Flood[playerid] = 2;
			return false;
		}
		else
		{
			format(string, sizeof(string), "[Pirdapiri Eteri] Stumari %s: %s", sendername, text);
			LSNews(0x00BFFFAA, string);
			//// Flood[playerid] = 2;
			return false;
		}
	}
	if(Tel[playerid] == 1)
	{
		if(PlayerInfo[playerid][pMuted] == 1) {SendErrorMessage(playerid, "Tkven Gadevt Chatis Bani!"); return false; }
		format(string, sizeof(string), "[Mobile] %s: %s", sendername, text);
		ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
		return false;
	}
	if(Mobile[playerid] != 999)
	{
		if(PlayerInfo[playerid][pMuted] == 1) {SendErrorMessage(playerid, "Tkven Gadevt Chatis Bani!"); return false; }
		format(string, sizeof(string), "[Mobile] %s: %s", sendername, text);
		ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
